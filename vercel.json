{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.yourbestseo.com" }],
      "destination": "https://yourbestseo.com/:path*",
      "statusCode": 301
    }
  ],
  "rewrites": [
    { "source": "/blog/category/(.*)", "destination": "/blog/index.html" },
    { "source": "/blog/author/(.*)", "destination": "/blog/index.html" },
    { "source": "/blog/page/(.*)", "destination": "/blog/index.html" }
  ]
}
